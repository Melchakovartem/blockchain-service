<div class="container">
  Block hash: <%= transaction.t_blockHash %><br>
  Block number: <%=link_to transaction.t_blockNumber, block_path(transaction.t_blockNumber) %><br>
  From: <%=link_to transaction.t_from, address_path(transaction.t_from) %><br>
  <% if transaction.t_to.nil? %>
    To: [ Contract <%=link_to transaction.t_contract_address, address_path(transaction.t_contract_address) %> Created ] <br>
  <% else %>
    To: <%= link_to transaction.t_to, address_path(transaction.t_to) %><br>
  <% end %>
  Gas: <%= transaction.t_gas.to_f %> Gwei<br>
  Value: <%= transaction.t_value.to_f/10**18 %><br>
  Gas price: <%= transaction.t_gasPrice.to_f/10**9 %> Gwei<br>
  Hash: <%= transaction.t_hash %>
 <div class="input"> Input: <%= transaction.t_input %></div>
  Nonce: <%= transaction.t_nonce %><br>
  Transaction index: <%= transaction.t_transactionIndex %>
</div>
